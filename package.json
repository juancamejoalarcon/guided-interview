{
  "name": "guided-interview",
  "private": false,
  "version": "0.0.35",
  "type": "module",
  "main": "./guided-interview.umd.js",
  "module": "./guided-interview.js",
  "types": "./types/GuidedInterview.d.ts",
  "exports": {
    ".": {
      "import": "./guided-interview.js",
      "require": "./guided-interview.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && npm run build-types && vite build --mode production",
    "build-demo": "tsc && vite build --mode demo --base=/guided-interview/demo",
    "build-types": "tsc -t es5 src/lib/GuidedInterview.ts --declaration --emitDeclarationOnly --downlevelIteration && npm run build-types-folder",
    "build-types-folder": "node build-types-folder.js",
    "preview": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.8",
    "@supercharge/strings": "^2.0.0",
    "mv": "^2.1.1",
    "nunjucks": "^3.2.4",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-transition-group": "^4.4.5",
    "rollup-plugin-react-scoped-css": "^0.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.1",
    "@types/nunjucks": "^3.2.2",
    "@types/react": "^18.0.27",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "jest": "^29.5.0",
    "sass": "^1.58.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
